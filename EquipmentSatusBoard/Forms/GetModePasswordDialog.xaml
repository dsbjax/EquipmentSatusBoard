<Window x:Class="EquipmentSatusBoard.Forms.GetModePasswordDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EquipmentSatusBoard.Forms"
        mc:Ignorable="d" WindowStyle="None" SizeToContent="WidthAndHeight" WindowStartupLocation="CenterScreen" Background="{x:Null}" AllowsTransparency="True">
    <Grid>
        <Rectangle Style="{StaticResource DialogbackGround}"/>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Top" Margin="20">
            <Label Name="title" Content="Enter Password" Style="{StaticResource DialogTitle}"/>
            <StackPanel Orientation="Horizontal" VerticalAlignment="Center" Margin="15">
                <PasswordBox Name="password" Width="200" VerticalAlignment="Center"/>
                <Button Name="enter" Content="Enter" Margin="15,0,0,0" Width="75" IsDefault="True" VerticalAlignment="Center" Click="EnterClick"/>
            </StackPanel>
        </StackPanel>
    </Grid>
</Window>
