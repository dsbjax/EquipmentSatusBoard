<UserControl x:Class="EquipmentSatusBoard.StatusBoardControl.StatusPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:EquipmentSatusBoard.StatusBoardControl"
             mc:Ignorable="d" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
    <Grid>
        <Image Name="backgroundImage" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="100"/>
        <DockPanel Name="page" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" LastChildFill="False"/>
    </Grid>
</UserControl>
