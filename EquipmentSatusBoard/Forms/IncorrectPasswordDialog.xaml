<Window x:Class="EquipmentSatusBoard.Forms.IncorrectPasswordDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EquipmentSatusBoard.Forms"
        mc:Ignorable="d"
        Background="{x:Null}" AllowsTransparency="True" WindowStyle="None" SizeToContent="WidthAndHeight" WindowStartupLocation="CenterScreen">
    <Grid>
        <Rectangle Style="{StaticResource DialogbackGround}"/>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Margin="20">
            <Label Content="Incorrect Password" Style="{StaticResource DialogTitle}" HorizontalAlignment="Center"/>
            <Label Content="Esc to close" HorizontalAlignment="Center"/>
        </StackPanel>
        <Button Name="close" IsCancel="True" Click="CloseClick" Background="{x:Null}" Foreground="{x:Null}" BorderBrush="{x:Null}"/>
    </Grid>
</Window>
